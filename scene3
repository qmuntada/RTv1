scene
{
	name(SCENE1)
	cam_pos(10 2 8)
	cam_dir(3 1 2)
	render(1600 900)
}

content
{
	object(light)
	{
		pos(1 1 5)
		power(25);
	}
	object(cone)
	{
		pos(5 2 -3)
	}
	object(cylinder)
	{
		size(100)
		pos(-2 0 -2)
	}
	object(plane)
	{
		rot(0 1 0)
		pos(0 -1 0)
	}
	object(plane)
	{
		rot(0 0 90)
		pos(0 0 -30)
	}
	object(sphere)
	{
		size(100)
		pos(-1 0 0)
	}
	object(sphere)
	{
		size(50)
		pos(0 0 0)
	}
}
